package com.generics;

public class PrintMyArray {

	public static void main(String[] args) {

		int[] a = { 1, 2, 3 };
		double[] b = { 1.1, 1.2, 1.3 };
		char[] c = { 'a', 'b' };
		toPrint(a);
		toPrintb(b);
	}

	private static void toPrintb(double[] b) {
		for (double i : b) {
			System.out.println(i);
		}
		
	}

	private static void toPrint(int[] a) {
		for (int i : a) {
			System.out.println(i);
		}

	}

}
